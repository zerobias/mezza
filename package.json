{
  "name": "mezza",
  "version": "0.2.0-rc1",
  "description": "An object switch-case. Selects the appropriate case from the object with variants",
  "main": "lib/index.js",
  "module": "es/index.js",
  "esnext": "es/index.js",
  "jsnext:main": "es/index.js",
  "typings": "index.d.ts",
  "scripts": {
    "clean": "rimraf lib/ && rimraf es/",
    "test": "cross-env BABEL_ENV=test jest --config jest.json",
    "build:cjs": "cross-env BABEL_ENV=commonjs babel src/ -d lib -s",
    "build:es": "babel src/ -d es -s",
    "build": "npm run build:cjs && npm run build:es",
    "watch": "npm run build:cjs -- --watch",
    "prepublish": "npm run clean && npm run build"
  },
  "author": "Zero Bias",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^20.0.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-es2015-block-scoping": "^6.24.1",
    "babel-plugin-transform-es2015-for-of": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-flow-comments": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "cross-env": "^5.0.1",
    "eslint": "^4.2.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsdoc": "^3.1.1",
    "flow-bin": "^0.49.1",
    "jest": "^20.0.4",
    "rimraf": "^2.6.1",
    "tslint": "^5.5.0",
    "typescript": "^2.3.4"
  },
  "files": [
    "src",
    "lib",
    "es",
    "flow-typed",
    "index.d.ts",
    "README.md"
  ],
  "bugs": {
    "url": "https://github.com/zerobias/mezza/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zerobias/mezza.git"
  },
  "keywords": [
    "pattern matching",
    "switch",
    "case"
  ],
  "quokka": {
    "babel": {
      "env": "commonjs"
    }
  }
}
