{
  "presets": [
  ],
  "plugins": [
    "transform-flow-comments",
    ["transform-object-rest-spread", { "useBuiltIns": true }],
    ["transform-es2015-for-of", {
      "loose": true
    }],
    ["transform-es2015-block-scoping", {
      "throwIfClosureRequired": true
    }]
  ],
  "env": {
    "commonjs": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ]
    },
    "test": {
      "plugins": [
        "transform-es2015-modules-commonjs"
      ],
      "sourceMaps": true,
      "retainLines": true
    }
  }
}